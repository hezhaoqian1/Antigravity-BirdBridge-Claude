{
  "$schema": "../gen/schemas/capabilities.json",
  "identifier": "main",
  "description": "Capabilities required for the main Antigravity Desktop window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:app:default",
    "core:event:default",
    "core:menu:default",
    "core:path:default",
    "core:resources:default",
    "core:tray:default",
    "core:window:default",
    "core:window:allow-set-title",
    "shell:allow-open"
  ]
}
